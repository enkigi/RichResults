{
    "name": "RichResults",
    "author": [
        "Encyc Team",
        "Enki"
    ],
    "url": "https://www.mediawiki.org/wiki/Extension:RichResults",
    "description": "Adds site-wide Organization JSON-LD for Google rich results and structured data."
    "version": "0.3.0",
    "license-name": "MIT",
    "type": "other",
    "requires": {
        "MediaWiki": ">= 1.35.0"
    },

    "AutoloadNamespaces": {
        "RichResults\\": "src/"
    },

    "HookHandlers": {
        "RichResults.main": {
            "class": "RichResults\\HookHandler"
        }
    },

    "Hooks": {
        "BeforePageDisplay": "RichResults.main"
    },

    "manifest_version": 2
}
